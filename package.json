    "gulp-notify": "^2.2.0",
    "gulp-pug-hyperscript": "^1.0.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.5.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.8",
    "iced-coffee-coverage": "^1.0.1",
    "iced-coffee-script": "^108.0.11",
    "istanbul": "^0.4.4",
    "js-beautify": "^1.6.3",
    "maquette": "^2.3.3",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.1",
